{
  "name": "datetime-picker",
  "version": "0.1.0",
  "description": "Web Component built with Stencil making date time picking easy. It wraps https://flatpickr.js.org",
  "module": "dist/esm/index.js",
  "main": "dist/datetimepicker.js",
  "unpkg": "dist/datetimepicker.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "classlist-polyfill": "1.2.0"
  },
  "devDependencies": {
    "@stencil/core": "~0.17.0",
    "@stencil/sass": "~0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarrvis/datetime-picker.git"
  },
  "author": "Micha≈Ç Kaliszewski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jarrvis/datetime-picker"
  },
  "homepage": "https://github.com/jarrvis/datetime-picker"
}
